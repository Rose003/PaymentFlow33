{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build && npm run prerender && npm run sitemap",
    "build:prerender": "tsc --noEmit && vite build && npm run sitemap && npm run prerender:with-server",
    "prerender:with-server": "concurrently \"npm run preview\" \"sleep 5 && npm run prerender\" --kill-others",
    "build:seo": "tsc --noEmit && vite build && npm run prerender && npm run sitemap && npm run verify-indexing",
    "prerender": "node scripts/prerender.js",
    "sitemap": "node scripts/generate-sitemap.js",
    "verify-indexing": "node scripts/verify-indexing.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "postbuild": "node scripts/postbuild.js"
  },
